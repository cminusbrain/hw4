#include <iostream>
#include <ctime>
#include <cmath>

int main()
{
    double x;
    std::cin >> x;
    if (x < 0)
    {
        std::cout << "f(x) = " << 0;
    }
    if (((int) x) % 2 == 0)
    {
        std::cout << "f(x) = " << 1;
