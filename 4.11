#include <iostream>
#include <ctime>

int main()
{
    int a, b;
    std::cin >> a >> b;
    if (a == b)
    {
        std::cout << "A = " << 0 << " " << "B = " << 0;
    }
    else if (a > b)
    {
        std::cout << "A = " << a << " " << "B = " << a;
    }
    else
        std::cout << "A = " << b << " " << "B = " << b;
    return 0;
}
