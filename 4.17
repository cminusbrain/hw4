#include <iostream>
#include <ctime>

int main()
{
    int a, b, c;
    std::cin >> a >> b >> c;
    if (b < a & c < b || b < c & a < b)
    {
        a = 2 * a;
        b = 2 * b;
        c = 2 * c;
    }
    else
        a = -a;
        b = -b;
        c = -c;
    std::cout << a << " " << b << " " << c;
    return 0;
}
